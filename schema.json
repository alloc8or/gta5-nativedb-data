{
	"type": "object",
	"patternProperties": {
		"^[A-Z_]+[A-Z0-9_]*$": {
			"type": "object",
			"patternProperties": {
				"^0x[0-9A-F]{16}$": {
					"type": "object",
					"properties": {
						"name": { "$ref": "#/$defs/native_name" },
						"jhash": {
							"type": "string",
							"pattern": "(^$|^0x[0-9A-F]{8}$)"
						},
						"comment": { "type": "string" },
						"params": {
							"type": "array",
							"items": {
								"type": "object",
								"properties": {
									"type": { "$ref": "#/$defs/type_name" },
									"name": {
										"type": "string",
										"pattern": "^[A-Za-z_]+[A-Za-z0-9_]*$"
									}
								},
								"required": ["type", "name"],
								"additionalProperties": false
							}
						},
						"return_type": { "$ref": "#/$defs/type_name" },
						"build": { "type": "string" },
						"unused": { "type": "boolean" },
						"old_names": {
							"type": "array",
							"minItems": 1,
							"uniqueItems": true,
							"items": { "$ref": "#/$defs/native_name" }
						}
					},
					"required": ["name", "jhash", "comment", "params", "return_type", "build"],
					"additionalProperties": false
				}
			},
			"additionalProperties": false
		}
	},
	"additionalProperties": false,
	"$defs": {
		"native_name": {
			"type": "string",
			"pattern": "^[A-Z_]+[A-Z0-9_]*$"
		},
		"type_name": {
			"type": "string",
			"pattern": "^[A-Za-z_]+[A-Za-z0-9_* ]*$"
		}
	}
}